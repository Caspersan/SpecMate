{
  "functions": {
    "api/anthropic/messages.js": {
      "maxDuration": 60
    },
    "api/logs/usage.js": {
      "maxDuration": 10
    },
    "api/logs/report.js": {
      "maxDuration": 10
    },
    "api/logs/contact.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/anthropic/messages",
      "destination": "/api/anthropic/messages.js"
    },
    {
      "source": "/api/logs/usage",
      "destination": "/api/logs/usage.js"
    },
    {
      "source": "/api/logs/report",
      "destination": "/api/logs/report.js"
    },
    {
      "source": "/api/logs/contact",
      "destination": "/api/logs/contact.js"
    }
  ]
}


